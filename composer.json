{
	"require": {
		"laravel/framework": "4.1.*",
        "iron-io/iron_mq": "dev-master"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
            "app/models",
            "app/services",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		],
        "psr-0": {
            "Lvlfr": "src"
        }
	},
	"scripts": {
		"pre-update-cmd": [
			"php artisan clear-compiled"
		],
		"post-install-cmd": [
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan optimize"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
